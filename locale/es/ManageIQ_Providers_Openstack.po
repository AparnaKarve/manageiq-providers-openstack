msgid "#{cpus} CPUs, #{ram} RAM, #{disk_size} Root Disk"
msgstr ""

msgid "Cannot start. Already on."
msgstr "No se puede iniciar. Ya está encendida."

msgid "Cannot stop. Already off."
msgstr "No se puede apagar. Ya está apagada."

msgid "Host ID cannot be nil"
msgstr "ID de host no puede ser nulo"

msgid "Host cannot be found"
msgstr "No se encuentra el host"

msgid "Migrate operation is not supported."
msgstr "No se admite la operación de migración."

msgid "No snapshots available for this VM"
msgstr "No hay instantáneas para esta MV"

msgid "Operation not supported"
msgstr "No se admite esta operación"

msgid "Provider stack is not ready to be updated, another operation is in progress."
msgstr "La pila del proveedor no está lista para ser actualizada; hay otra operación en curso."

msgid "Subnet ID cannot be nil"
msgstr "La ID de subred no puede ser nula"

msgid "Subnet cannot be found"
msgstr "No se puede encontrar la subred"

msgid "Tenant mapping is disabled on the Provider"
msgstr "El mapeo del inquilino está inhabilitado en el proveedor"

msgid "Tenant mapping is supported only when CloudTenant exists on the CloudManager"
msgstr "El mapeo del inquilino es compatible solo cuando CloudTenant existe en el CloudManager"

msgid "The Cloud Network is not connected to an active %{table}"
msgstr "La red de nube no está conectada a una %{table} activa"

msgid "The Flavor is not connected to an active %{table}"
msgstr "La clase no está conectada con una %{table} activa"

msgid "The Floating Ip is not connected to an active %{table}"
msgstr "La IP flotante no está conectada a una %{table} activa"

msgid "The Image is not connected to an active %{table}"
msgstr "La imagen no está conectada a una %{table} activa"

msgid "The Instance cannot be resized, current state has to be active or shutoff."
msgstr "La instancia no se puede volver a dimensionar, el estado actual tiene que estar activo o desactivado."

msgid "The Keypair is not connected to an active %{table}"
msgstr "El par de claves no está conectada a una %{table} activa"

msgid "The Network Router is not connected to an active %{table}"
msgstr "El enrutador de red no está conectado a una %{table} activa"

msgid "The Security Group is not connected to an active %{table}"
msgstr "El grupo de seguridad no está conectado a una %{table} activa"

msgid "The VM is not powered on"
msgstr "La MV no está encendida"

msgid "The subnet has an active %{table}"
msgstr "La subred tiene una %{table} activa"

msgid "The subnet is not connected to an active %{table}"
msgstr "La subred no está conectada a una %{table} activa"

msgid "There are no %{floating_ips} available to this %{instance}."
msgstr "No hay %{floating_ips} disponibles para esta %{instance}."

msgid "There are no %{security_groups} available to this %{instance}."
msgstr "No exiten %{security_groups} disponibles para esta %{instance}."

msgid "This %{instance} does not have any associated %{floating_ips}"
msgstr "Esta %{instance} no tiene ningún %{floating_ips} asociado"

msgid "This %{instance} does not have any associated %{security_groups}"
msgstr "Esta %{instance} no tiene  ningún %{security_groups} asociado"

msgid "Unknown constraint %{constraint}"
msgstr "Restricción desconocida %{constraint}"

msgid "cloud_volume cannot be nil"
msgstr "cloud_volume no puede ser nulo"

msgid "ems cannot be found"
msgstr "No se encuentra EMS"

msgid "ems cannot be nil"
msgstr "EMS no puede ser nulo"

msgid "ext_management_system cannot be nil"
msgstr "ext_management_system no puede ser nulo"

msgid "not connected to ems"
msgstr "no está conectado a EMS"
